{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    },
    {
      "src": "apps/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/apps/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/vercel_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9",
    "FLASK_ENV": "production",
    "RUNTIME": "cloud",
    "VERCEL": "1"
  }
}
