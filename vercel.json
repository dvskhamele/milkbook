{
  "version": 2,
  "builds": [
    {
      "src": "apps/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "apps/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "js/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/pos/?",
      "dest": "/apps/dairy-pos-billing-software-india.html"
    },
    {
      "src": "/collection/?",
      "dest": "/apps/collection.html"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/$1"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key",
    "SUPABASE_SERVICE_ROLE": "@supabase_service_role"
  }
}
