{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/apps/(.*)",
      "dest": "/apps/$1"
    },
    {
      "src": "/pos",
      "dest": "/index.html"
    },
    {
      "src": "/collection",
      "dest": "/apps/collection.html"
    },
    {
      "src": "/farmers",
      "dest": "/apps/farmer-management-milk-collection-centers.html"
    },
    {
      "src": "/customers",
      "dest": "/apps/customer-ledger-udhar-tracking-dairy.html"
    },
    {
      "src": "/inventory",
      "dest": "/apps/inventory.html"
    },
    {
      "src": "/reports",
      "dest": "/apps/reports-dashboard.html"
    },
    {
      "src": "/settings",
      "dest": "/apps/settings.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.+\\.html)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.(css|js|json|png|jpg|jpeg|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
